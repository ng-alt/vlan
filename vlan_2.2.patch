+    
+    kfree(dev->vlan_dev);
+    VLAN_FMEM_DBG("dev->vlan_dev free, addr: %p\n", dev->vlan_dev);
+    dev->vlan_dev = NULL;
+ 
+    kfree(dev);
+    VLAN_FMEM_DBG("device free, addr: %p\n", dev);
+    dev = NULL;
+ 
+    return;
+ }
