+ int vlan_dev_set_dflt_vid(char* dev_name, unsigned short vid) {
+    struct device* dev = dev_get(dev_name);
+ 
+    if (dev) {
+       if (!dev->vlan_dev) { /* can't put a dflt ID on a vlan device */
+          if ((vid > 0) && (vid <= 0xFFF)) {
+             dev->default_vid = (vid & 0xFFF);
+             return 0;
+          }
+       }
+    }
+    return -EINVAL;
+ }
